{
	"compilerOptions": {
		"outDir": "./built",
		"allowJs": true,
		"target": "ES6",
      "module": "commonjs",
        "strict": true,
      "moduleResolution": "Node",
      "allowSyntheticDefaultImports": true,
      "esModuleInterop": true
	},
	"include": [
		"./src/**/*"
	],
  "exclude": ["node_modules", ".idea"],
}